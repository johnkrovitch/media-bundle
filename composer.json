{
    "name": "johnkrovitch/media-bundle",
    "description": "Media Bundle for Cms",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "johnkrovitch",
            "email": "arnaudfrezet@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "ext-gd": "*",
        "johnkrovitch/orm-pack": "^2.0",
        "lag/string-utils": "^1.2",
        "liip/imagine-bundle": "^2.2",
        "pagerfanta/doctrine-collections-adapter": "^3.1",
        "pagerfanta/doctrine-orm-adapter": "^3.1",
        "symfony/form": "^3.4|^4.4|^5.2",
        "symfony/framework-bundle": "^3.4|^4.4|^5.2",
        "symfony/twig-bundle": "^3.4|^4.4|^5.2",
        "symfony/validator": "^3.4|^4.4|^5.2",
        "symfony/yaml": "^3.4|^4.4|^5.2"
    },
    "require-dev": {
        "ext-json": "*",
        "behat/behat": "^3.8",
        "behat/mink-extension": "^2.3",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^0.12.2",
        "phpunit/phpunit": "^8.4",
        "robertfausk/behat-panther-extension": "^1.0",
        "symfony/phpunit-bridge": "^3.4|^4.4|^5.2",
        "symfony/webpack-encore-bundle": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "JK\\MediaBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JK\\MediaBundle\\Tests\\": "tests/phpunit/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "component-dir": "components",
        "sort-packages": true
    }
}
