{
    "name": "johnkrovitch/media-bundle",
    "description": "Media Bundle for Cms",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "johnkrovitch",
            "email": "arnaudfrezet@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "johnkrovitch/orm-pack": "^1.0",
        "lag/string-utils": "^1.2",
        "liip/imagine-bundle": "^2.2",
        "symfony/form": "^3.0|^4.0|5.0",
        "symfony/framework-bundle": "^3.0|^4.0|5.0",
        "symfony/validator": "^3.0|^4.0|^5.0",
        "symfony/yaml": "^3.0|^4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.2",
        "phpunit/phpunit": "^8.4",
        "symfony/phpunit-bridge": "^3.0|^4.1.12|^5.0",
        "symfony/webpack-encore-bundle": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "JK\\MediaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JK\\MediaBundle\\Tests\\": "tests/MediaBundle/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "component-dir": "components",
        "sort-packages": true
    }
}
